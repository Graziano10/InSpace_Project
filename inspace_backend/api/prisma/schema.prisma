// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  password String?
}

model Travel {
  id    Int     @id @default(autoincrement())
  name  String?
  bookingDate DateTime?
  planet String?
  email String? @unique
}